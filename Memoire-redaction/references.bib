@article{strassel_geopolitical_2018,
	title = {The geopolitical challenges of university globalization},
	volume = {168},
	issn = {0338-487X},
	abstract = {University globalization refers to the phenomenon behind the rising number of international students and exchange programs worldwide, as well as the increase in institution offshoring. This concept, however, also conveys specific views on higher education. The latter have led to new norms that are used by various interest groups to standardize the organization and functioning of institutions on the basis of a model inspired by the Anglo-American “research university.” These norms, conveyed in particular by world rankings, are shaping the programs aimed at accelerating the transformation of higher education in Europe, such as the Exzellenzinitiative in Germany, or the Idex in France. They are gradually reducing the specificities of the national models and are tending to lessen the ability of governments to focus their higher education policy on their own objectives.},
	pages = {9},
	number = {1},
	journaltitle = {Hérodote},
	author = {Strassel, Christophe},
	date = {2018},
	note = {{OCLC}: 7961557997},
}

@article{strassel_les_2018,
	title = {Les enjeux géopolitiques de la mondialisation universitaire},
	volume = {N° 168},
	issn = {0338-487X},
	url = {https://www.cairn.info/revue-herodote-2018-1-page-9.htm},
	abstract = {La mondialisation universitaire désigne le phénomène qui voit les effectifs étudiants et les échanges internationaux d’étudiants augmenter partout dans le monde, ainsi que les délocalisations d’établissements s’intensifier. Toutefois, la mondialisation universitaire véhicule également des représentations sur l’enseignement supérieur. Celles-ci sont à l’origine de normes qui sont utilisées par divers groupes de pression pour uniformiser l’organisation et le fonctionnement des établissements sur la base d’un modèle qui est celui de l’« université de recherche » anglo-saxonne. Ces normes sont véhiculées notamment par les classements internationaux et structurent les programmes qui visent à accélérer les transformations de l’enseignement supérieur en Europe : Exzellenzinitiative en Allemagne, Idex en France. Elles réduisent progressivement les spécificités des modèles nationaux et tendent à réduire la capacité des États à orienter leur politique d’enseignement supérieur sur la base d’objectifs propres.},
	pages = {9--38},
	number = {1},
	journaltitle = {Herodote},
	author = {Strassel, Christophe},
	urldate = {2019-02-10},
	date = {2018-06-14},
